<div class="container-fluid">
    <div class="well clearfix">
      <div class="centered title">
        <h1 style=" font-size:25px; text-align: center;color: #22BDC8;">   
          Gineco-Obstétricos
        </h1>
      </div>
    <div class="row">
      <div class="col-md-2">
        <label class="control-label" for="firstname">Ciclo Menstrual</label>
        <%= f.text_field :b_ciclo_menstrual, class: 'form-control', placeholder: 'Ciclo Menstrual', :id => "ciclo", :value => 'no aplica' %>

      </div>

      <div class="col-md-2">
        <label class="control-label" for="firstname">Eco</label>
        <%= f.text_field :b_eco, class: 'form-control', placeholder: 'Eco', :id => "eco", :value => 'no aplica' %>
      </div>

      <div class="col-md-2">
        <label class="control-label" for="firstname">FUM</label>
        <%= f.text_field :b_fum, class: 'form-control', placeholder: 'Fum', :id => "fum", :value => 'no aplica' %>
      </div>

      <div class="col-md-2" >
        <label class="label" for="firstname">Menarquia</label>
        <%= f.text_field :b_menarquia, class: 'form-control', placeholder: 'Menarquia', :id => "menarquia", :value => 'no aplica'  %>
      </div>







