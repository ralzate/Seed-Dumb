<div class="container-fluid">
    <div class="row">
        <div class="panel panel-default modal-dialog">
            <div class="panel-body">
 
     <%= link_to "1. Informacion General", paciente_historia_clinica_steps_path(@paciente, @historia_clinica, HistoriaClinica.form_steps.first), :class=>"btn btn-info color_blanco mrg" %>

